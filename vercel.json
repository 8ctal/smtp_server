{
    "version": 2,
    "functions": {
        "api/*.py": {
            "maxDuration": 5,
            "memory": 1024
        }
    },
    "routes": [
        {
            "src": "/health",
            "dest": "/api/app.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/app.py"
        }
    ]
} 